var imgs = $('img').not('#logo img');
imgs.addClass('thumbnail');
imgs.addClass('intense');
$('#logo img').addClass('thumbnail');
$('a').click(function(e){
    var el = $(this), t = el.attr('href'), hash = t ? t.toString() : '';
    if (!t) return e.preventDefault();
    //if (t[0]==='#') e.preventDefault();
    if (t[0]!=='#' || t.length<2) return;// console.log('DISCARD LINK');
    // t = $(t).offset();
    // to = t ? t.top||0 : 0;
    if (hash) {
        $('body').animate({
            scrollTop: $(t).css('top')
        }, 100).promise().then(function() {
          console.log("runs once!",t)
            if (el.hasClass('menu')) $('#menu-close').click();
            location.hash = hash;
        });
            return false;
    } else return; //console.log('Error with offset',to,t,hash);
});
window.init = function(){
  Intense( imgs );
};
$('#top').css('opacity:.84');
window.changeLang = function(l){
    contactForm = '<div class="btn-group pull-right"><a data-lang="contact" href="#suggestions" class="btn btn-warning chinaBut btn-sm"> E&E </a><a data-lang="contact" href="#" class="btn btn-info btn-sm" data-toggle="modal" data-target="#contactModal">'+langs.contact[l]+'</a></div><br><br>';
    var text = $('[data-lang]'), jq,em,attr;
    $('#logo').attr('href','mind/?page_id=9&lang='+(l==='zh' ? 'en' : l));
    $('#qbevents').attr('href','mind/?p=view&lang='+(l==='zh' ? 'en' : l));
    for (var i = 0; i < text.length; i++) {
        jq = $(text[i]);
        attr = jq.attr('data-lang');
                //else {
                    var t = jq.html().split('  ').join('').split('\n').join(' ').split("'").join("\\'"), href, show;
                    // if (langs[attr]) console.log('\t, '+attr+': {\n\t\ten: \''+(langs[attr]['en']||t)+'\'\n\t\t, es: \''+(langs[attr]['es']||t)+'\'\n\t\t, ca: \''+(langs[attr]['ca'].split("'").join("\\'")||t)+'\'\n\t\t, ru: \''+(langs[attr]['ru']||t)+'\'\n\t\t, zh: \''+(langs[attr]['zh']||t)+'\'\n\n\t\t, doc: \''+t+'\'\n\t}');
                    // else 
                    if (!langs[attr]) console.log('\t, '+attr+': {\n\t\ten: \''+t+'\'\n\t\t, es: \''+t+'\'\n\t\t, ca: \''+t+'\'\n\t\t, ru: \''+t+'\'\n\t\t, zh: \''+t+'\'\n\t\t}');
                //    if (langs[attr] && langs[attr][l]) jq.html(langs[attr][l]);
                //}
                if (langs[attr] && langs[attr][l]) jq.html(langs[attr][l]);
            };
            text = $('[data-content]');
            for (var i = 0; i < text.length; i++) {
                jq = $(text[i]);
                attr = jq.data('content');
                href = jq.data('href');
                if (href){
                    show = contactForm.replace('#suggestions',href);
                } else show = contactForm;
                if (langs[attr] && langs[attr][l]) jq.attr('data-content',langs[attr][l]+show);
                attr = jq.attr('title');
                if (langs[attr] && langs[attr][l]) {
                    jq.attr('title',langs[attr][l]);
                    jq.attr('data-original-title',langs[attr][l]);
                }
            };
            text = $('#services a');
            text.popover({ placement: 'bottom', trigger: 'click', html: true, width: '164px' });
            jq = $('#expop');
            em = $('#empop');
            var mb = 
            jq.popover({ placement: mobile ? 'bottom' : 'right', trigger: 'click', html: true });
            em.popover({ placement: mobile ? 'bottom' : 'left', trigger: 'click', html: true });
            jq.click();
            em.click();
            text.click();
            jq = $('.popover');
            //jq.css('background-image', '-webkit-gradient(linear,left top,left bottom,color-stop(0%,#FCEEC1),color-stop(100%,#E4BF46))');
            jq.css('opacity',.81);
            jq.css('color','black');
            text.click();
            $('#expop').click();
            em.click();
            function closeLast(){
                if (last===this) {
                    last = undefined;
                    return;
                }
                if (last) last.click();
                last = this;
            }
            text.bind('click',closeLast);
            $('#expop').bind('click',closeLast);
            em.bind('click',closeLast);
        }
          window.language = navigator.language.split('-')[0];
          var  but=$('#langsBut');
          for (var i = 0; i < langs.languages.length; i++) {
            but.append('<li><a class="menu" style="color:black" href="javascript:;" onclick="changeLang(\''+langs.languages[i]+'\')">'+langs.names[i]+'</a></li>');
            };
            qui.html(htqui);
            if (langs.languages.indexOf(language)!=-1){
                changeLang(language);
            } else changeLang('en');
        window.hidePins = setTimeout(function(){
            init();
            //$($('#pins').parent().children()[0]).toggle('fast');
            $('.more').click(function(e){
              var el = $(this);
              if (el.attr('href')==='#contact') return;
              $(el.parent().children()[0]).toggle('fast');
              var i = el.find('i');
              i.removeClass('fa-plus');
              //i.addClass('fa-send');
              i.html('');
                      //el.text('Contact us  ');
                      bodyS.animate({
                        scrollTop: $(window).scrollTop() + bodyS.height()/2
                    });
                      //$('#pin').src('');
                      //$('#pin').attr('src','http://assets.pinterest.com/js/pinit.js');
                      setTimeout(function(){
                          el.attr('href','#contact');
                      },1000);
                  });
            $('[data-pin-log]')
            .attr('href','http://www.pinterest.com/quilibriumbcn/')
            .attr('target','_blank').click(function(e){ e.stopImmediatePropagation() });
        },2800);
        $('.carousel').carousel({ interval: 3600 });
    </script>
    
